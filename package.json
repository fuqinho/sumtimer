{
  "name": "sumtimer",
  "version": "0.0.1",
  "description": "A timer app to record daily activities.",
  "productName": "Sumtimer",
  "author": "Naoki Fukino <naoki.fukino@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:emulator": "cross-env VITE_USE_EMULATOR=1 firebase emulators:exec 'npm run dev' --ui --project demo-sumtimer --import=./firebase_export --export-on-exit",
    "build": "run-p type-check build-only",
    "build:emulator": "cross-env VITE_USE_EMULATOR=1 npm run build",
    "preview": "vite preview",
    "deploy": "firebase deploy",
    "test:unit": "vitest --root src/",
    "test:e2e": "start-server-and-test 'vite preview' :4173 'cypress run --e2e'",
    "test:e2e:emulator": "firebase emulators:exec 'npm run test:e2e' --ui --project demo-sumtimer --import=./cypress/firebase/data/",
    "test:e2e:dev": "start-server-and-test 'vite --port 4173' :4173 'cypress open --e2e'",
    "test:e2e:dev:emulator": "cross-env VITE_USE_EMULATOR=1 firebase emulators:exec 'npm run test:e2e:dev' --ui --project demo-sumtimer --import=./cypress/firebase/data/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "dotenv": "^16.0.3",
    "firebase": "^9.13.0",
    "pinia": "^2.0.23",
    "quasar": "^2.10.1",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@quasar/vite-plugin": "^1.2.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/vue": "^6.6.1",
    "@types/chrome": "^0.0.203",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "cypress": "^11.1.0",
    "cypress-firebase": "^2.2.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "firebase-admin": "^11.2.0",
    "happy-dom": "^7.6.6",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "1.32.12",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^3.2.3",
    "vitest": "^0.24.5",
    "vue-tsc": "^1.0.9"
  }
}
